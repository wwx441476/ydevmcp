{
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "tsc": "^2.0.4",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "name": "@wsx/ydevmcp",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "yesdev-mcp-server": "dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    "./*": {
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/*"
      ]
    }
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",

    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "test": "jest",
    "pub": "npm publish --registry=https://repo.yyrd.com/artifactory/api/npm/ynpm-private/",
    "build:esm": "mkdir -p dist/esm && echo '{\"type\": \"module\"}' > dist/esm/package.json && tsc --project tsconfig.prod.json",
    "build:cjs": "mkdir -p dist/cjs && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json && tsc --project tsconfig.cjs.json"
  },
  "test": {
    "unit": "echo \"No unit tests yet\""
  },
  "keywords": [
    "mcp",
    "ydev",
    "client",
    "modelcontextprotocol",
    "sdk"
  ],
  "files": [
    "dist"
  ],
  "author": "wangsxb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwx441476/ydevmcp/issues"
  },
  "description": "mcp client for ydev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwx441476/ydevmcp.git"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  }
}
